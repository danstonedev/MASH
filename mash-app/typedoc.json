{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": [
        "src"
    ],
    "entryPointStrategy": "expand",
    "out": "docs/api",
    "name": "IMU Connect API Documentation",
    "readme": "README.md",
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeExternals": true,
    "includeVersion": true,
    "categorizeByGroup": true,
    "categoryOrder": [
        "Math",
        "Calibration",
        "Biomechanics",
        "Device",
        "UI",
        "*"
    ],
    "navigation": {
        "includeCategories": true,
        "includeGroups": true
    },
    "plugin": [],
    "exclude": [
        "**/node_modules/**",
        "**/*.test.ts",
        "**/*.spec.ts",
        "**/tests/**"
    ]
}